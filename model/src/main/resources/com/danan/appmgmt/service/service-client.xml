<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">


	<bean id="appMgmt"
		class="org.springframework.remoting.jaxws.JaxWsPortProxyFactoryBean">
		<property name="serviceInterface" value="org.danan.appmanagment.AppMgmt"></property>
		<property name="wsdlDocumentUrl" value="classpath:wsdl/appManagment.wsdl"></property>
		<property name="serviceName" value="AppMgmtService"></property>
		<property name="portName" value="appMgmtPort"></property>
	</bean>


</beans>
